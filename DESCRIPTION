Package: FastStepGraph
Title: FastStepGraph: Fast Sparse Precision Matrix Estimation
Version: 0.1.0
Authors@R: c(
    person("Juan G.", "Colonna", , "juancolonna@icomp.ufam.edu.br", role = c("cre", "aut"),
